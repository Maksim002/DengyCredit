<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PinEntryEditText">
        <attr name="pinAnimationType" format="enum">
            <enum name="popIn" value="0" />
            <enum name="fromBottom" value="1" />
            <enum name="none" value="-1" />
        </attr>
        <attr name="pinCharacterMask" format="string" />
        <attr name="pinRepeatedHint" format="string" />
        <attr name="pinLineStroke" format="dimension" />
        <attr name="pinLineStrokeSelected" format="dimension" />
        <attr name="pinCharacterSpacing" format="dimension" />
        <attr name="pinTextBottomPadding" format="dimension" />
        <attr name="pinLineColors" format="color" />
        <attr name="pinBackgroundDrawable" format="reference" />
        <attr name="pinBackgroundIsSquare" format="boolean" />
    </declare-styleable>

    <attr name="title_text"/>
    <attr name="content_text"/>
    <attr format="reference" name="bg_drawable_regular"/>
    <attr format="reference" name="bg_drawable_expanded"/>
    <attr format="reference" name="arrow_drawable"/>
    <attr name="title_text_color"/>
    <attr name="title_text_color_expanded"/>
    <attr format="dimension" name="title_text_size"/>
    <attr format="reference" name="title_font"/>
    <attr format="dimension" name="content_text_size"/>
    <attr name="content_text_color"/>
    <attr name="link_text_color"/>
    <attr format="boolean" name="underline_link"/>
    <attr format="reference" name="content_font"/>
    <attr format="integer" name="expand_duration"/>
    <attr format="dimension" name="panel_padding"/>
    <attr format="dimension" name="content_padding"/>
</resources>